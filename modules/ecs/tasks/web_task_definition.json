[
  {
    "name": "ms-order",
    "image": "${image}",
    "portMappings": [
      {
        "containerPort": 80,
        "hostPort": 80
      },
      {
        "containerPort": 3000,
        "hostPort": 3000
      }
    ],
    "memory": 300,
    "networkMode": "awsvpc",
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${log_group}",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "ms-order"
      }
    },
    "environment": [
      {
        "name": "DB_USER",
        "value": "${db_username}"
      },
      {
        "name": "DB_PASSWORD",
        "value": "${db_password}"
      },
      {
        "name": "AWS_REGION",
        "value": "${aws_region}"
      },
      {
        "name": "DB_NAME",
        "value": "${database_name}"
      },
      {
        "name": "SECRET_KEY_JWT_TOKEN",
        "value": ""
      },
      {
        "name": "PORT",
        "value": "3000"
      },
      {
        "name": "AWS_SESSION_TOKEN",
        "value": "${session_token_aws}"
      },
      {
        "name": "AWS_ACCESS_KEY",
        "value": "${access_key_aws}"
      },
      {
        "name": "AWS_SECRET",
        "value": "${secret_aws}"
      },
      {
        "name": "DB_HOST",
        "value": "${db_host}"
      },
      {
        "name": "AWS_OUTPUT_ORDER_PAYMENT_SQS_URL",
        "value": "${output_orderpayment_sqs_url}"
      },
      {
        "name": "AWS_OUTPUT_ORDER_CONFIRMED_SQS_URL",
        "value": "${output_orderconfirmed_sqs_url}"
      },
      {
        "name": "AWS_INPUT_PAYMENT_PROCESSED_SQS_URL",
        "value": "${input_paymentprocessed_sqs_url}"
      },
      {
        "name": "AWS_INPUT_QUEUE_FINISHED_SQS_URL",
        "value": "${input_queue_finished_sqs_url}"
      },
      {
        "name": "MSG_POLLING_INTERVAL",
        "value": "${sqs_polling_interval}"
      },
      {
        "name": "AWS_MESSAGE_GROUP",
        "value": "${sqs_message_group}"
      }
    ],
    "healthCheck": {
      "command": [
        "CMD-SHELL",
        "curl -f http://localhost:3000/api-docs/#/ || exit 1"
      ],
      "interval": 30,
      "timeout": 5,
      "retries": 3,
      "startPeriod": 60
    }
  }
]